        <a href="{{ path_for("user.management") }}" class="list-group-item list-group-item-info">
            <img src="{{ base_url() }}/images/admin-blue.png"/>&emsp;<b>Users</b>
        </a>
        <a href="{{ path_for("log.info") }}" class="list-group-item list-group-item-info">
            <img src="{{ base_url() }}/images/logfile.png"/>&emsp;<b>Log</b>
