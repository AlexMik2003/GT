<div class="list-group menu">
    <a href="{{ path_for("dashboard") }}" class="list-group-item list-group-item-info">
        <img src="{{ base_url() }}/images/icon_dash.png"/>&emsp;<b>Dashboard</b>
    </a>
    <a href="{{ path_for("clients.management") }}" class="list-group-item list-group-item-info">
        <img src="{{ base_url() }}/images/clients-blue.png"/>&emsp;<b>Clients</b>
