    <a href="{{ path_for("network.management") }}" class="list-group-item submenu">
        <img src="{{ base_url() }}/images/arrow_blue.png"/>&nbsp;<span id="submenu">Network</span>
    </a>
    <a href="{{ path_for("vlan.management",{"page":1}) }}" class="list-group-item submenu">
        <img src="{{ base_url() }}/images/arrow_blue.png"/>&nbsp;<span id="submenu">Vlans</span>
    </a>
    <a href="{{ path_for("peers.management") }}" class="list-group-item submenu">
        <img src="{{ base_url() }}/images/arrow_blue.png"/>&nbsp;<span id="submenu">Peers</span>
