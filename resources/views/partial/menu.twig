    <a href="{{ path_for("area.management") }}" class="list-group-item list-group-item-info">
        <img src="{{ base_url() }}/images/area-blue.png"/>&emsp;<b>Area</b>
    </a>
    <a href="{{ path_for("map.info") }}" class="list-group-item list-group-item-info">
        <img src="{{ base_url() }}/images/map-blue.png"/>&emsp;<b>Map</b>
    </a>
    <a href="#" class="list-group-item list-group-item-info">
        <img src="{{ base_url() }}/images/wiki-blue.png"/>&emsp;<b>Wiki</b>
    </a>
